{% extends "base.html.twig" %}

{% block title %}Création d'un produit Bijoux
{% endblock %}

{% form_theme form 'bijoux/_collection.html.twig' %}


{% block body %}

	<div class="container">
		<h1>Créer un nouveaux Bijoux</h1>

		{{ form_start(form) }}

		{{ form_widget(form) }}

		<button type="submit" class="btn btn-primary">Créer un nouveaux Bijoux</button>

		{{ form_end(form) }}
	</div>


{% endblock %}


{% block javascripts %}
	<script src="/js/bijoux.js"></script>
{% endblock %}
